{
    "name": "itsmedudes/laravel-whatsapp",
    "description": "Laravel WhatsApp Business + Meta Graph client with multi-tenant token support.",
    "type": "laravel-package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "whatsapp",
        "meta",
        "whatsapp-business",
        "graph-api"
    ],
    "authors": [
        {
            "name": "itsmedudes",
            "homepage": "https://github.com/itsmedudes"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelWhatsapp\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelWhatsapp\\\\MetaServiceProvider"
            ],
            "aliases": {
                "Meta": "LaravelWhatsapp\\\\Facades\\\\Meta"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelWhatsapp\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/itsmedudes/laravel-whatsapp/issues",
        "source": "https://github.com/itsmedudes/laravel-whatsapp"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
